              <goal>wget</goal>
            </goals>
            <configuration>
              <skip>${hashicorp.terraform.download.skip}</skip>
              <unpack>true</unpack>
              <checkSignature>true</checkSignature>
              <url>${hashicorp.terraform.url}</url>
